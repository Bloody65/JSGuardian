{
  "manifest_version": 2,
  "name": "jsGuardian",
  "version": "0.13.1",

  "icons": { "128": "filter.png" },

  "permissions": ["tabs", "<all_urls>", "storage"],

  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["get_text_script.js"]
    }
  ],
  "options_ui": {
    "page": "options.html"
  },
  "storage": {
    "managed_schema": "schema.json"
  }
}
