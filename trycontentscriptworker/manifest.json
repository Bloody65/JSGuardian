{
  "manifest_version": 2,

  "name": "test web worker content script",
  "version": "0.1",
  "author": "serv-inc",

  "permissions": ["tabs", "<all_urls>"],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./load_worker.js"],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "./LogWorker.js"
  ],
  "content_security_policy": "script-src 'self' https://example.com 'chrome-extension'; object-src 'self' 'chrome-extension'"
}
